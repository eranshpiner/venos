{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "id": "/orderLog",
    "title": "Order Log",
    "description": "The record of an accepted order",
    "type": "object",
    "additionalProperties": false,    
    "required": [
        "orderId",
        "transactionId",
        "transactionTimestamp", 
        "brandId",
        "brandLocationId",
        "posVendorId",
        "posId",        
        "posResponseStatus",
        "posResponseCode",
        "orderStatus"
    ],
    "properties": {
        "orderId": {
            "type": "number"                
        },       
        "transactionId": {
            "type": "string"                
        },  
        "transactionTimestamp": {
            "type": "number"                
        },              
        "brandId": {
            "type": "string"                
        },
        "brandLocationId": {
            "type": "string"                
        },        
        "posVendorId": {
            "type": "string"                
        },
        "posId": {
            "type": "string"                
        },        
        "posResponseStatus": {
            "type": "string"
        },
        "posResponseCode": {
            "type": "integer"
        },
        "orderStatus": {
            "type": "integer"
        }           
                        
    }
}