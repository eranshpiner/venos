{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "id": "/orderLog",
    "title": "Order Log",
    "description": "The record of an accepted order",
    "type": "object",
    "additionalProperties": false,    
    "required": [
        "orderId",
        "transactionId",
        "businessId",
        "businessType",
        "posId",        
        "posVendorId",
        "posVendorName",
        "responseCode",
        "responseStatus"
    ],
    "properties": {
        "orderId": {
            "type": "string"                
        },
        "transactionId": {
            "type": "string"                
        },        
        "businessId": {
            "type": "string"                
        },
        "businessType": {
            "type": "string"                
        },
        "posId": {
            "type": "string"                
        },        
        "posVendorId": {
            "type": "string"                
        },
        "posVendorName": {
            "type": "string"                
        },
        "responseStatus": {
            "type":"string"
        },
        "responseCode": {
            "type": "number"
        }   
                        
    }
}